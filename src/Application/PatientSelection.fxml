<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.PatientSelectionController">
  <Text layoutX="14.0" layoutY="83.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Search:">
    <font>
      <Font size="24.0"/>
    </font>
  </Text>
  <TextField layoutX="124.0" layoutY="61.0" prefHeight="25.0" prefWidth="352.0"/>
  <Button fx:id="selectPatientButton" layoutX="476.0" layoutY="344.0" mnemonicParsing="false"
    onAction="#patientSelect" text="Select Patient"/>
  <TableView fx:id="patientTableView" layoutX="124.0" layoutY="129.0" prefHeight="200.0"
    prefWidth="352.0">
    <columns>
      <TableColumn fx:id="columnName" prefWidth="117.0" text="Name"/>
      <TableColumn fx:id="columnAge" prefWidth="122.0" text="Age"/>
      <TableColumn fx:id="columnGender" prefWidth="112.0" text="Gender"/>
    </columns>
  </TableView>
</AnchorPane>
